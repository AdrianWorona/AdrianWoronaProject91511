@model IEnumerable<AdrianWoronaProject91511.Models.Film>

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

    <table>
        <thead>
            <tr>
                <th>Tytuł filmu</th>
                <th>Reżyser</th>
                <th>Opis</th>
                <th>Cena</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var film in Model)
            {
                <tr>
                    <td>@film.Title</td>
                    <td>@film.Director</td>
                    <td>@film.Desc</td>
                    <td>@film.Price</td>
                </tr>
            }
        </tbody>
    </table>